{
    "name": "kemnhanonline",
    "version": "0.2.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=development craco start",
        "build": "NODE_ENV=production GENERATE_SOURCEMAP=false craco build",
        "test": "NODE_ENV=test craco test",
        "eject": "craco eject",
        "all": "rm -rf node_modules && yarn cache clean && yarn",
        "lint": "NODE_ENV=development BABEL_ENV=development eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "NODE_ENV=development BABEL_ENV=development eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.8.2",
        "ajv": "^8.17.1",
        "ajv-keywords": "^5.1.0",
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "dayjs": "^1.11.13",
        "lodash": "^4.17.13",
        "nprogress": "^0.2.0",
        "react": "^18.3.1",
        "react-activity": "^1.2.2",
        "react-bootstrap": "^1.4.3",
        "react-data-table-component": "^7.0.0-alpha-5",
        "react-dom": "^18.3.1",
        "react-helmet-async": "^2.0.5",
        "react-lazy-load-image-component": "^1.5.1",
        "react-redux": "^9.1.2",
        "react-router": "^6.30.0",
        "react-router-dom": "6.30.1",
        "react-router-hash-link": "^2.3.1",
        "react-scripts": "5.0.1",
        "react-toastify": "^10.0.6",
        "react-youtube": "^7.13.0",
        "reactstrap": "^8.9.0",
        "reactstrap-date-picker": "^0.0.11",
        "redux": "^4.0.5",
        "styled-components": "^5.3.11",
        "web-vitals": "^0.2.4",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.28.0",
        "@babel/traverse": "^7.25.7",
        "@craco/craco": "^7.1.0",
        "@types/jest": "^30.0.0",
        "@types/lodash": "^4.17.13",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.10",
        "@types/react-dom": "^19.1.7",
        "@types/react-lazy-load-image-component": "^1.6.4",
        "@types/react-redux": "^7.1.34",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-router-hash-link": "^2.4.9",
        "@types/youtube": "^0.1.2",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "prettier-plugin-organize-imports": "^4.2.0",
        "tsconfig-paths-webpack-plugin": "^4.2.0",
        "typescript": "^5.9.2"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
